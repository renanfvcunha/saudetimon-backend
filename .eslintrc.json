{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard",
        "plugin:prettierx/standardx",
        "plugin:prettierx/@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettierx"
    ],
    "rules": {
        "prettierx/options": "warn",
        "camelcase": "off"
    }
}
